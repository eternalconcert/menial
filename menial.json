{
    "hosts": {
        "*:3434": {
            "handler": "redirect",
            "root": "",
            "target": "http://localhost:3435/",
            "permanent": false
        },
        "*:3435": {
            "handler": "file",
            "root": "deployment/default/",
            "errorpages": "deployment/default/errorpages/"
        }
    },
    "logger": "console",
    "loglevel": "debug"
}
