{
    "hosts": {
        "default.localhost:8000": {
            "handler": "file",
            "root": "deployment/default/",
            "errorpagesdir": "deployment/default/errorpages/"
        },
        "*:8000": {
            "handler": "file",
            "root": "testdata/testhost/",
            "errorpagesdir": "deployment/default/errorpages/"
        },
        "*:8080": {
            "handler": "file",
            "root": "testdata/threaded/",
            "errorpagesdir": "deployment/default/errorpages/"
        },
        "*:8888": {
            "handler": "python",
            "root": "testdata/testhost/pyresponse.py",
            "errorpagesdir": "deployment/default/errorpages/"
        }
    },
    "loglevel": "debug",
    "logfilepath": "/tmp/",
    "logger": "console"
}
