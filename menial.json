{
    "hosts": {
        "*:8000": {
            "handler": "python",
            "root": "testdata/testhost/pyresponse.py",
            "errorpages": "deployment/default/errorpages/",
            "additionalheaders": "Access-Control-Allow-Origin: *\n"
        },
        "*:8080": {
            "handler": "file",
            "root": "deployment/default/",
            "errorpages": "deployment/default/errorpages/"
        }
    },
    "iplogging": false,
    "logger": "console",
    "logfilepath": "/tmp/",
    "loglevel": "debug"
}
