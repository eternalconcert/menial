{
    "hosts": {
        "*:8000": {
            "responder": "file",
            "root": "testdata/testhost/",
            "errorpagesdir": "testdata/testhost/errorpages/"
        },
        "*:8080": {
            "responder": "file",
            "root": "testdata/threaded/",
            "errorpagesdir": "testdata/testhost/errorpages/"
        },
        "*:8888": {
            "responder": "python",
            "root": "testdata/testhost/pyresponse.py",
            "errorpagesdir": "testdata/testhost/errorpages/"
        }
    },
    "loglevel": "debug",
    "logfilepath": "/tmp/",
    "logger": "console"
}
