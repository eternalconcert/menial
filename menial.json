{
    "hosts": {
        "localhost:8000": {
            "handler": "file",
            "root": "deployment/default/"
        },
        "proxy.localhost:8000": {
            "handler": "proxy",
            "upstream": "https://eternalconcert.de:443"
        },
        "menial.localhost:8000": {
            "handler": "file",
            "staticdir": "static/",
            "root": "website/build/"
        },
        "selenium.localhost:8000": {
            "handler": "file",
            "root": "selenium/reports/",
            "defaultdocument": "report.html"
        },
        "files.localhost:8000": {
            "handler": "file",
            "root": "/home/",
            "dirlisting": true
        },
        "tmmk.localhost:8000": {
            "handler": "file",
            "root": "../tmmk/build/"
        },
        "python.localhost:9999": {
            "handler": "python",
            "root": "pyresponse:application"
        },
        "eternalconcert.localhost:9999": {
            "handler": "python",
            "root": "wsgi:application"
        }
    },
    "iplogging": true,
    "debug": true,
    "logger": "console",
    "logfilepath": "/tmp/",
    "loglevel": "debug",
    "staticdir": "resources/static/",
    "resources": "resources/",
    "ssl": {
        "4433": {
            "on": false,
            "cert": "cert.pem",
            "key": "key.pem"
        }
    }
}
